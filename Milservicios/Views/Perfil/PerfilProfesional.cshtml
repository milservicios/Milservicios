@{
    string[] palabras = new string[8];
    palabras[0] = "Alberto";
    palabras[1] = "de Marcos Molina";
    palabras[2] = "679213821";
    palabras[3] = "alberto@gmail.com";
    palabras[4] = "Hombre";
    palabras[5] = "25";
    palabras[6] = "Informatico de la UAH";
    palabras[7] = "Corredor del henares";
}
<!DOCTYPE html>
<html>
<head>
    <link rel="stylesheet" href="~/Content/Perfiles/PerfilProfesional.css" />
    <script type="text/javascript"
            src="http://maps.google.com/maps/api/js?sensor=false"></script>
</head>
<body>
    <!-- CABECERA WEB -->
    <header id="PP-Header">
        <h1 id="PP-TituloPrincipal">Perfil de Profesional</h1>
    </header>
    <!-- CUADRO PRINCIPAL DE LA PAGINA WEB PERFIL FAMILIA-->
    <section id="PP-CuadroCentral">
        <h3 id="PP-Informacion">Informacion General</h3>
        <br />
        <!-- Cuadrado A -->
        <section id="PP-CuadradoA">
            <!-- Foto de perfil-->
            <section>
                <img id="PP-Foto" src="~/imagenes/foto.png" />
            </section>
            <!-- Cuadrado C -->
            <section id="PP-CuadradoC">
                <a href="/Perfil/ModificarPerfilProfesional"><section id="PP-Link1">Editar perfil</section></a>
                <a href="/GestionAnuncios/TipoAnuncios"><section id="PP-Link2">Crea anuncio</section></a>
                <a href="/GestionAnuncios/VistaAnuncios"><section id="PP-Link3">Ver anuncios</section></a>
                <a href="/Perfil/ModificarPerfilFamilia"><section id="PP-Link4">Ver reservas</section></a>
            </section>
        </section>
        <!-- Cuadrado B -->
        <section id="PP-CuadradoB">
            <!-- Cuadrado Izquierdo-->
            <section id="PP-Cuadrado1">
                <!-- Primera fila - Nombre -->
                <section>
                    <section id="PP-NombreT">Nombre</section>
                    <section id="PP-NombreC">@palabras[0]</section>
                    <br />
                </section>
                <br />
                <!-- Segunda fila - Apellidos -->
                <section>
                    <section id="PP-ApellidoT">Apellidos</section>
                    <section id="PP-ApellidoC">@palabras[1]</section>
                    <br />
                </section>
                <br />
                <!-- Tercera fila - Telefono -->
                <section>
                    <section id="PP-TelefonoT">Telefono</section>
                    <section id="PP-TelefonoC">@palabras[2]</section>
                    <br />
                </section>
                <br />
                <!-- Cuarta fila - Email -->
                <section>
                    <section id="PP-EmailT">Email</section>
                    <section id="PP-EmailC">@palabras[3]</section>
                    <br />
                </section>
                <br />
                <!-- Quinta fila - Sexo -->
                <section>
                    <section id="PP-SexoT">Sexo</section>
                    <section id="PP-SexoC">@palabras[4]</section>
                    <br />
                </section>
                <br />
                <!-- Sexta fila - Edad -->
                <section>
                    <section id="PP-EdadT">Edad</section>
                    <section id="PP-EdadC">@palabras[5]</section>
                    <br />
                </section>
                <br />
                <!-- Septima fila - Descripcion -->
                <section>
                    <section id="PP-DescripcionT">Descripcion</section>
                    <section id="PP-DescripcionC">@palabras[6]</section>
                    <br />
                </section>
                <br />
                <!-- Octava fila - Zona -->
                <section>
                    <section id="PP-ZonaT">Zona</section>
                    <section id="PP-ZonaC">@palabras[7]</section>
                    <br />
                </section>
                <br />
            </section>            
            <!-- Cuadrado Derecho-->
            <section id="PP-Cuadrado2">
                <h3 id="PP-MapaTitulo">Mapa de localizacion</h3>
                <section id="PP-CuadroMap">
                    <script>
                        function initMap() {
                            //Mapa
                            var map = new google.maps.Map(document.getElementById('PP-CuadroMap'), {
                                zoom: 17,
                                center: { lat: 40.513034, lng: -3.348728 },
                                mapTypeControl: true,
                                mapTypeControlOptions: {
                                    style: google.maps.MapTypeControlStyle.HORIZONTAL_BAR,
                                    position: google.maps.ControlPosition.TOP_CENTER
                                },
                                zoomControl: true,
                                zoomControlOptions: {
                                    position: google.maps.ControlPosition.LEFT_CENTER
                                },
                                scaleControl: true,
                                streetViewControl: true,
                                streetViewControlOptions: {
                                    position: google.maps.ControlPosition.LEFT_TOP
                                },
                                fullscreenControl: true
                            });                            
                            var marker = new google.maps.Marker({
                                position: { lat: 40.513034, lng: -3.348728 },
                                map: map,
                                title: 'Alberto'
                            });
                        }
                    </script>
                    <script async defer
                            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAZQp616kSgYGzMCGI7lXIRWF8G3Qsmvg0&callback=initMap">
                    </script>
                </section>
            </section>
        </section>
    </section>
</body>
</html>